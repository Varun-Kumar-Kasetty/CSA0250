//Subraction in matrix
#include<stdio.h>
#include<math.h>
#include<stdlib.h>
int main()
{
	int a[3][3], b[3][3], c[3][3], i, j;
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			printf("Enter the elements of a = ");
			scanf("%d", &a[i][j]);
		}
		printf("\n");
	}
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			printf("Enter the elements of b = ");
			scanf("%d", &b[i][j]);
		}
		printf("\n");
	}
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			c[i][j]=a[i][j]-b[i][j];
		}
	}
	printf("\n");
	printf("The subracted matrix = ");
	printf("\n");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			printf("%d ", c[i][j]);
		}
		printf("\n");
	}
	return 0;
}
