#include<stdio.h>
#include<conio.h>
int fact(int n);
int main()
{
	int n;
	printf("Enter the value of 'n' = ");
	scanf("%d", &n);
	printf("The Factorial of '%d' = %d", n, fact(n));
	return 0;
}
int fact(int n)
{
	int fact(n);
	if ( n >= 1)
	{
		return n * fact( n - 1 );
	}
	else
	{
		return 1;
	}
}
