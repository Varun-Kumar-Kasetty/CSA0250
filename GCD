#include<stdio.h>
int main()
{
	int gcd, i, a, b;
	printf("Enter the value of 'a' = ");
	scanf("%d", &a);
	printf("Enter the value of 'b' = ");
	scanf("%d", &b);
	for(i=1 ; i<=a && i<=b ; i++)
	{
		if(a%i==0 && b%i==0)
		{
			gcd = i;
		}
	}
	printf("GCD of %d and %d is %d", a, b, gcd);
	return 0;
}
