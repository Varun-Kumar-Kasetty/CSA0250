#include<stdio.h>
int main()
{
	int i, j, a[3][3];
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			printf("Enter the elements in A = ");
			scanf("%d", &a[i][j]);
		}
		printf("\n");
	}
	printf("\n");
	printf("A = \n");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			printf("%d ", a[i][j]);
		}
		printf("\n");
	}
	printf("\n");
	printf("A^T = \n");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			printf("%d ", a[j][i]);
		}
		printf("\n");
	}
	return 0;
}
